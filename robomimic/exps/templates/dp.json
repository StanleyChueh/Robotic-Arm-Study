{
    "algo_name": "diffusion",
    "experiment": {
        "name": "diffusion_test",
        "validate": false,
        "logging": {
            "terminal_output_to_txt": true,
            "log_tb": true,
            "log_wandb": false,
            "wandb_proj_name": "diffusion_debug"
        },
        "save": {
            "enabled": true,
            "every_n_seconds": null,
            "every_n_epochs": 50,
            "epochs": [],
            "on_best_validation": false,
            "on_best_rollout_return": true,
            "on_best_rollout_success_rate": true
        },
        "epoch_every_n_steps": 100,
        "validation_epoch_every_n_steps": 10,
        "env": null,
        "additional_envs": null,
        "render": false,
        "render_video": true,
        "keep_all_videos": false,
        "video_skip": 5,
        "rollout": {
            "enabled": true,
            "n": 50,
            "horizon": 400,
            "rate": 50,
            "warmstart": 0,
            "terminate_on_success": true
        }
    },
    "train": {
        "data": "datasets/mujoco_lift_demo.hdf5",
        "output_dir": "../diffusion_trained_models",
        "num_data_workers": 0,
        "hdf5_cache_mode": "none",
        "hdf5_use_swmr": false,
        "hdf5_load_next_obs": true,
        "hdf5_normalize_obs": true,
        "hdf5_filter_key": null,
        "hdf5_validation_filter_key": null,
        "seq_length": 1,
        "pad_seq_length": true,
        "frame_stack": 1,
        "pad_frame_stack": true,
        "dataset_keys": [
            "actions",
            "rewards",
            "dones",
            "robot0_eef_pos",
            "robot0_eef_quat",
            "robot0_gripper_qpos",
            "object",
            "robot0_eye_in_hand_image"
        ],
        "goal_mode": null,
        "cuda": true,
        "batch_size": 2, 
        "num_epochs": 2000,
        "seed": 1
    },
    "algo": {
        "diffusion": {
            "enabled": true,
            "steps": 100,
            "noise_schedule": "cosine",
            "beta_start": 0.0001,
            "beta_end": 0.02,
            "sampling_timesteps": 100,
            "predict_x0": true
        },
        "optim_params": {
            "policy": {
                "optimizer_type": "adam",
                "learning_rate": {
                    "initial": 0.0001,
                    "decay_factor": 0.1,
                    "epoch_schedule": [],
                    "scheduler_type": "multistep"
                },
                "regularization": {
                    "L2": 0.0
                }
            }
        },
        "hidden_dim": 1024,  
        "num_layers": 6,
        "num_heads": 8,
        "dropout": 0.1,
        "learning_rate": 0.0001,
        "weight_decay": 0.01,
        "denoising_network": {
            "type": "transformer",
            "context_length": 10,
            "embed_dim": 512,
            "num_layers": 6,
            "num_heads": 8,
            "emb_dropout": 0.1,
            "attn_dropout": 0.1,
            "block_output_dropout": 0.1,
            "sinusoidal_embedding": false,
            "activation": "gelu"
        },
        "policy_architecture": {
            "use_conditioning": true,
            "conditioning_type": "concat",
            "input_norm": true
        }
    },
    "observation": {
        "modalities": {
            "obs": {
                "low_dim": [
                    "robot0_eef_pos",
                    "robot0_eef_quat",
                    "robot0_gripper_qpos",
                    "object"
                ],
                "rgb": ["robot0_eye_in_hand_image"], 
                "depth": [],
                "scan": []
            },
            "goal": {
                "low_dim": [],
                "rgb": [],
                "depth": [],
                "scan": []
            }
        },
        "encoder": {
            "low_dim": {
                "core_class": null,
                "core_kwargs": {},
                "obs_randomizer_class": null,
                "obs_randomizer_kwargs": {}
            },
            "rgb": {
                "core_class": "VisualCore",  
                "core_kwargs": {
                    "conv_layers": [
                        {"out_channels": 32, "kernel_size": 8, "stride": 4},
                        {"out_channels": 64, "kernel_size": 4, "stride": 2},
                        {"out_channels": 64, "kernel_size": 3, "stride": 1}
                    ],
                    "fc_layers": [512],  
                    "activation": "relu",
                    "flatten": true
                },
                "obs_randomizer_class": null,
                "obs_randomizer_kwargs": {}
            },
            "depth": {
                "core_class": "VisualCore",
                "core_kwargs": {},
                "obs_randomizer_class": null,
                "obs_randomizer_kwargs": {}
            },
            "scan": {
                "core_class": "ScanCore",
                "core_kwargs": {},
                "obs_randomizer_class": null,
                "obs_randomizer_kwargs": {}
            }
        }
    },
    "meta": {
        "hp_base_config_file": null,
        "hp_keys": [],
        "hp_values": []
    }
}
